{
    "python.terminal.launchArgs": [],
    "sarif-viewer.connectToGithubCodeScanning": "off",
    "cSpell.words": [
        "mbtcp"
    ],
    "python.analysis.autoImportCompletions": true,
    "python.analysis.typeCheckingMode": "basic",
